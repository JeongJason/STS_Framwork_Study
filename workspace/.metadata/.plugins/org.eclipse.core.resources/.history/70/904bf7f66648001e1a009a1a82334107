package com.example.board.domain.vo;

import lombok.AllArgsConstructor;
import lombok.Data;

@Data
@Log4j
public class Criteria {
	private int pageNum;
	private int amount;
	
	public Criteria() {
		this(1, 10);
		log.info("들어옴!");
	}

	public Criteria(int pageNum, int amount) {
		this.pageNum = pageNum;
		this.amount = amount;
	}
	
	
}
